{
	"name": "black-lion",
	"version": "1.0.0",
	"description": "Some experimentation with got and a large api and database",
	"author": {
		"name": "Alex Page",
		"email": "alex@alexpage.com.au",
		"url": "http://alexpage.com.au/"
	},
	"contributors": [
		{
			"name": "Alex Page",
			"email": "alex@alexpage.com.au",
			"url": "http://alexpage.com.au"
		}
	],
	"scripts": {
		"start": "npm run build && node dist/init.js",
		"build": "babel src --out-dir dist --presets=env",
		"build:verbose": "npm run build && node dist/init.js --verbose",
		"watch": "nodemon --watch src --exec npm run start",
		"watch:verbose": "nodemon --watch src --exec npm run build:verbose"
	},
	"engines": {
		"node": "8.8.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alex-page/black-lion"
	},
