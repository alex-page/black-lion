	},
	"homepage": "https://github.com/alex-page/black-lion#readme",
	"dependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"got": "^8.0.0"
	},
	"devDependencies": {
		"nodemon": "^1.12.1"
	}
}
